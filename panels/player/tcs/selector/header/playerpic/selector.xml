<panel navigation_container="true">
	<container id="SUBP">
		<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true" inset="2" />
		<layout class="stick_to_sides_attachment" alignment="all" inset="0" />
	</container>
	
	<widget class="popup_button" id="popv" auto_size="horizontal" height="15" click_event="msis" appearance="tcs/buttons/header/button" fixed="true">
		<layout class="stick_to_sides_attachment" alignment="top,right" inset="0" />
	</widget>

	<record id="default_properties" late_loading="true"/>

	<!-- FACE + BACK KIT ZOOMED -->
	<container class="client_object_viewer_xml_panel" file="player/tcs/selector/header/playerpic/default" id="defa" >
		<translation id="title" type="use" value="Face Back Kit (Zoomed)" />
	</container>

	<!-- FACE ONLY -->
	<container class="client_object_viewer_xml_panel" file="player/tcs/selector/header/playerpic/faceonly" id="face">
		<translation id="title" type="use" value="Face Only" />
	</container>

  <!-- FACE WITH KIT BG -->
	<container class="client_object_viewer_xml_panel" file="player/tcs/selector/header/playerpic/my_face_with_bg" id="fabg">
		<translation id="title" type="use" value="Face Only with Background" />
	</container>

	<!-- FACE + FULL BACK KIT -->
	<container class="client_object_viewer_xml_panel" file="player/tcs/selector/header/playerpic/back_full_dummy" id="bkzm" valid_for_unemployed_player="false" >
		<translation id="title" type="use" value="Face Back Kit (Full)" />
	</container>

	<!-- FACE + FULL FRONT KIT -->
	<container class="client_object_viewer_xml_panel" file="player/tcs/selector/header/playerpic/frontkit" id="fron" valid_for_unemployed_player="false" >
		<translation id="title" type="use" value="Face Front Kit (Full)" />
	</container>

  <!-- BACK KIT ZOOMED -->
	<container class="client_object_viewer_xml_panel" file="player/tcs/selector/header/playerpic/backkit" id="back" >
		<translation id="title" type="use" value="Back Kit Only (Zoomed)" />
	</container>
	
	<!-- Combined 
	<container class="client_object_viewer_xml_panel" file="player/tcs/selector/header/playerpic/combined" id="comb">
		<translation id="title" type="use" value="Combined" />
	</container> -->

</panel>